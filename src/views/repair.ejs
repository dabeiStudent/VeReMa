<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Repair Service</title>
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <!-- style css -->
    <link rel="stylesheet" href="css/style.css">
    <!-- Responsive-->
    <link rel="stylesheet" href="css/responsive.css">
    <!-- fevicon -->
    <link rel="icon" href="/icon/batman-icon.png" type="image/gif" />
    <!-- Scrollbar Custom CSS -->
    <link rel="stylesheet" href="css/jquery.mCustomScrollbar.min.css">
    <!-- Tweaks for older IEs-->
    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css"
        media="screen">
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
</head>

<body>
    <!-- header -->
    <header>
        <!-- header inner -->
        <div class="header">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-3 col logo_section">
                        <div class="full">
                            <div class="center-desk">
                                <div class="logo">
                                    <a href="/index.ejs"><img src="/images/logo1.png" style="border-radius: 10%;"
                                            alt="#" /></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-9">
                        <nav class="navigation navbar navbar-expand-md navbar-dark ">
                            <button class="navbar-toggler" type="button" data-toggle="collapse"
                                data-target="#navbarsExample04" aria-controls="navbarsExample04" aria-expanded="false"
                                aria-label="Toggle navigation">
                                <span class="navbar-toggler-icon"></span>
                            </button>
                            <div class="menu-bar">
                                <div class="collapse navbar-collapse" id="navbarsExample04">
                                    <ul class="navbar-nav mr-auto">
                                        <%if(role=="admin"){%>
                                            <li class="nav-item">
                                                <a class="nav-link" href="/manager.ejs">Manager</a>
                                            </li>
                                            <%}%>
                                                <li class="nav-item ">
                                                    <a class="nav-link" href="/index.ejs"> Home </a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" href="/about.ejs">About</a>
                                                </li>
                                                <li class="nav-item active">
                                                    <a class="nav-link" href="/furnitures.ejs">Products</a>
                                                    <div class="sub-nav">
                                                        <ul>
                                                            <li><a class="nav-link" href="/vproducts.ejs/1">Vehicle
                                                                    products</a>
                                                            </li>
                                                            <li><a class="nav-link" href="/repair.ejs">Repair
                                                                    Service</a>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" href="/contact.ejs">Contact Us</a>
                                                </li>
                                                <li class="nav-item">
                                                    <a class="nav-link" href="/chat.ejs">Chat here</a>
                                                </li>
                                                <%if(token !=null ){%>
                                                    <li class="nav-item d_none">
                                                        <a class="nav-link" href="/yourprofile.ejs"><i
                                                                class="fa fa-user-circle padd_right"
                                                                aria-hidden="true"></i>
                                                            Hello, <%=name%> (<%=role%>)</a>
                                                        <a class="nav-link" href="/logout.ejs">Logout</a>
                                                    </li>
                                                    <%}else{%>
                                                        <li class="nav-item d_none">
                                                            <a class="nav-link" href="/signin.ejs"><i
                                                                    class="fa fa-user-circle padd_right"
                                                                    aria-hidden="true"></i>Login</a>
                                                            <a class="nav-link" href="/signin.ejs">To join us</a>
                                                        </li>
                                                        <%}%>
                                                            <li class="nav-item d_none">
                                                                <a class="nav-link" href="#"><i class="fa fa-search"
                                                                        aria-hidden="true"></i></a>
                                                            </li>
                                    </ul>
                                </div>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div class="prod">
        <div class="addproduct">
            <div class="rounded p-4 rounded mb-4">
                <%if(role=="admin"){%>
                    <div id="addp">
                        <!-- <strong><a href="/addprod.ejs">Add new item</a></strong> -->
                    </div>
                    <%}%>
            </div>
            <div class="rounded p-4 rounded mb-4">
                <%if(role=="admin"){%>
                    <div id="addp">
                        <!-- <strong><a href="/updateprod.ejs">Update item</a></strong> -->
                    </div>
                    <%}%>
            </div>
        </div>
        <div class="addproductleft">
            <div class="rounded p-4 rounded mb-4">
                <div id="cateprod">
                    <h1>Categories: </h1>
                    <% if(service !=null){%>
                        <% for(let i=0; i< service.length; i++){%>
                            <a href="#">
                                <%=service[i].ten%>
                            </a><br>
                            <%}}%>
                </div>
            </div>
        </div>
        <div class="messcate">
            <h1 style="font-style: normal;">
                Xin chào
            </h1>
        </div>
        <div class="row mb-5">
            <% if(service !=null){%>
                <% for(let i=0; i< service.length; i++){%>
                    <div class="col-sm-6 col-lg-4 mb-4" data-aos="fade-up">
                        <div id="pchange" class="block-4 text-center border">
                            <figure class="block-4-image">
                                <img src="images/banner2.jpg" alt="Image placeholder" width="100px" height="100px"
                                    class="img-fluid">
                            </figure>
                            <div class="block-4-text p-4">
                                <p class="text-primary font-weight-bold">
                                    Dịch vụ: <%=service[i].mo_ta%>
                                </p><br>
                            </div>
                        </div>
                    </div>
                    <%}%>
                        <%}%>
        </div>
    </div>
    <footer id="contact">
        <div class="copyright">
            <div class="container">
                <div class="row">
                    <div class="col-md-12" id="copyright">
                        <p>Copyright 2023 All Right Reserved By VeReMa</p>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </footer>
    <!-- Javascript files-->
    <script src="js/jquery.min.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.bundle.min.js"></script>
    <script src="js/jquery-3.0.0.min.js"></script>
    <!-- sidebar -->
    <script src="js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="js/custom.js"></script>
</body>

</html>