<!DOCTYPE html>
<html lang="en">

<head>
    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>Manager</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- bootstrap css -->
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <!-- style css -->
    <link rel="stylesheet" href="css/style.css">
    <!-- Responsive-->
    <link rel="stylesheet" href="css/responsive.css">
    <!-- fevicon -->
    <link rel="icon" href="/icon/batman-icon.png" type="image/gif" />
    <!-- Scrollbar Custom CSS -->
    <link rel="stylesheet" href="css/jquery.mCustomScrollbar.min.css">
    <!-- Tweaks for older IEs-->
    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css"
        media="screen">
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
</head>
<!-- body -->

<body class="main-layout">
    <!-- header -->
    <header>
        <!-- header inner -->
        <div class="header">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-xl-3 col-lg-3 col-md-3 col-sm-3 col logo_section">
                        <div class="full">
                            <div class="center-desk">
                                <div class="logo">
                                    <a href="index.ejs"><img src="images/logo.png" alt="#" /></a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-9 col-lg-9 col-md-9 col-sm-9">
                        <nav class="navigation navbar navbar-expand-md navbar-dark ">
                            <button class="navbar-toggler" type="button" data-toggle="collapse"
                                data-target="#navbarsExample04" aria-controls="navbarsExample04" aria-expanded="false"
                                aria-label="Toggle navigation">
                                <span class="navbar-toggler-icon"></span>
                            </button>
                            <div class="collapse navbar-collapse" id="navbarsExample04">
                                <ul class="navbar-nav mr-auto">
                                    <%if(role=="admin"){%>
                                        <li class="nav-item active">
                                            <a class="nav-link" href="manager.ejs">Manager</a>
                                        </li>
                                        <%}%>
                                            <li class="nav-item ">
                                                <a class="nav-link" href="index.ejs"> Home </a>
                                            </li>
                                            <li class="nav-item">
                                                <a class="nav-link" href="about.ejs">About</a>
                                            </li>
                                            <li class="nav-item">
                                                <a class="nav-link" href="furnitures.ejs">Products</a>
                                            </li>
                                            <li class="nav-item">
                                                <a class="nav-link" href="contact.ejs">Contact Us</a>
                                            </li>
                                            <%if(token !=null ){%>
                                                <li class="nav-item d_none">
                                                    <a class="nav-link" href="logout.ejs"><i
                                                            class="fa fa-user-circle padd_right"
                                                            aria-hidden="true"></i>Logout</a>
                                                    <a class="nav-link" href="/yourprofile.ejs"><i>Hello, <%=name%> (
                                                                <%=role%> )</a>
                                                </li>
                                                <%}else{%>
                                                    <li class="nav-item d_none">
                                                        <a class="nav-link" href="signin.ejs"><i
                                                                class="fa fa-user-circle padd_right"
                                                                aria-hidden="true"></i>Login</a>
                                                    </li>
                                                    <%}%>
                                                        <li class="nav-item d_none">
                                                            <a class="nav-link" href="#"><i class="fa fa-search"
                                                                    aria-hidden="true"></i></a>
                                                        </li>
                                </ul>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- end header inner -->
    <!-- end header -->
    <!-- All account -->
    <br>
    <div class="classify">
        <h1>TÀI KHOẢN NHÂN VIÊN</h1>
        <table id="customers">
            <tr>
                <th>Mã TK</th>
                <th>Tên TK</th>
                <th>Mật Khẩu</th>
                <th>Quyền</th>
                <th>Xử Lí</th>
            </tr>
            <% for(var i=0; i< dataUser.length;i++) {%>
                <%if(dataUser[i].quyen=="admin" ){%>
                    <tr>
                        <td>
                            <%= dataUser[i].ma_tk %>
                        </td>
                        <td>
                            <%= dataUser[i].ten_tk %>
                        </td>
                        <td>
                            <%= dataUser[i].mat_khau %>
                        </td>
                        <td>
                            <%= dataUser[i].quyen %>
                        </td>
                        <td>
                            <a href="/detail/user/'<%= dataUser[i].ten_tk %>'" target="_self">Detail</a>
                            <a href="/update/user/'<%= dataUser[i].ten_tk %>'" target="_self">Update</a>
                            <button type="button">Remove</button>
                        </td>
                    </tr>
                    <% }} %>
        </table><br><br>
        <h1>TÀI KHOẢN KHÁCH</h1>>
        <table id="customers">
            <tr>
                <th>Mã TK</th>
                <th>Tên TK</th>
                <th>Mật Khẩu</th>
                <th>Quyền</th>
                <th>Xử Lí</th>
            </tr>
            <% for(var i=0; i< dataUser.length;i++) {%>
                <%if(dataUser[i].quyen=="kh" ){%>
                    <tr>
                        <td>
                            <%= dataUser[i].ma_tk %>
                        </td>
                        <td>
                            <%= dataUser[i].ten_tk %>
                        </td>
                        <td>
                            <%= dataUser[i].mat_khau %>
                        </td>
                        <td>
                            <%= dataUser[i].quyen %>
                        </td>
                        <td>
                            <a href="/detail/user/'<%= dataUser[i].ten_tk %>'" target="_self">Detail</a>
                            <a href="/update/user/'<%= dataUser[i].ten_tk %>'" target="_self">Update</a>
                            <button type="button">Remove</button>
                        </td>
                    </tr>
                    <% }} %>
        </table><br><br>
    </div>
    <!--  footer -->
    <footer id="contact">
        <div class="copyright">
            <div class="container">
                <div class="row">
                    <div class="col-md-12" id="copyright">
                        <p>Copyright 2023 All Right Reserved By VeReMa</p>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </footer>
    <!-- end footer -->
    <!-- Javascript files-->
    <script src="js/jquery.min.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.bundle.min.js"></script>
    <script src="js/jquery-3.0.0.min.js"></script>
    <!-- sidebar -->
    <script src="js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="js/custom.js"></script>
</body>

</html>